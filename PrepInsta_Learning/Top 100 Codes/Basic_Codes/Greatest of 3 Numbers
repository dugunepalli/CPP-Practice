#include<bits/stdc++.h>

using namespace std;
int main()
{
    int Number1,Number2,Number3;
    cout<<"Enter a Number 1: ";
    cin>>Number1;
    
    cout<<"Enter a Number 2: ";
    cin>>Number2;
    
    cout<<"Enter a Number 3: ";
    cin>>Number3;
    
    if(Number1>Number2 && Number1>Number3)
    {
        cout<<"Number "<<Number1<<" is greatest"<<endl;
    }
    else if(Number2>Number1 && Number1>Number3)
    {
        cout<<"Number "<<Number2<<" is greatest"<<endl;
    }
    else
    {
        cout<<"Number "<<Number3<<" is greatest"<<endl;
    }
}
