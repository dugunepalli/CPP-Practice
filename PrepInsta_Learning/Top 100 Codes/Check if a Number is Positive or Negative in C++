#include<bits/stdc++.h>

using namespace std;
int main()
{
    int Number;
    cout<<"Enter a Number: ";
    cin>>Number;
    
    if(Number>0)
    {
        cout<<"The number is Positive"<<endl;
    }
    else if(Number==0)
    {
        cout<<"The number is Zero"<<endl;
    }
    else
    {
        cout<<"The number is Negative"<<endl;
    }
}
