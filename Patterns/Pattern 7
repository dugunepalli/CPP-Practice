/*
 *To Design a Patter
        ex:
            12345
            12345
            12345
            12345
            12345
            
    here; 
        5 is Number Given By the User
 */

#include<bits/stdc++.h>
#include<iostream>
using namespace std;

int main()
{
    int Number;
    cout<<"Enter a Number: ";
    cin>>Number;
    
    for(int Loop=Number;Loop>0;Loop--)
    {
        for(int Inner=1;Inner<=Number;Inner++)
        {
            cout<<Inner;
        }
        cout<<endl;
    }
}
