/*
 *To Design a Patter
        ex:
            11111
            22222
            33333
            44444
            55555
 */

#include<bits/stdc++.h>
#include<iostream>
using namespace std;

int main()
{
    int Number;
    cout<<"Enter a Number: ";
    cin>>Number;
    
    for(int Loop=1;Loop<=Number;Loop++)
    {
        for(int Inner=1;Inner<=Number;Inner++)
        {
            cout<<Inner;
        }
        cout<<endl;
    }
}
